{
	"compilerOptions": {
		"checkJs": true,
		"allowJs": true,
		"forceConsistentCasingInFileNames": true,
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"noEmit": true,
		"noUnusedParameters": true,
		"lib": ["ESNext"],
		"esModuleInterop": true,
		"strict": true,
		"target": "ESNext",
		"types": ["node"]
	}
}
